create database lib;
use lib;
create table livros (
	id_livros int primary key auto_increment,
    nome_autor varchar (200),
    nome varchar (200),
    genero varchar (200),
    paginas int
    
);

create table login (
	id_user int primary key auto_increment,
    email varchar(255),
    password varchar(255)
);

PORTA=5002
MYSQL_HOST=localhost
MYSQL_USER=root
MYSQL_PASSWORD=1234
MYSQL_DATABASE=lib
